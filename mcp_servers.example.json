{
  "servers": [
    {
      "name": "filesystem",
      "transport": "stdio",
      "kind": "filesystem",
      "command": ["<your-filesystem-mcp-server-cmd>", "--root", "/absolute/path/to/workspace"]
    },
    {
      "name": "fetch",
      "transport": "http",
      "kind": "fetch",
      "url": "http://127.0.0.1:9001"
    }
  ]
}


